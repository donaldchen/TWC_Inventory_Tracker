%div{:class => "strip"}
%header{:class => "row header"}
  %div{:class => "col-2"}
    = link_to image_tag('back_btn.png', :class => "back_arrow"), root_path, :id => "back_btn"
  %div{:class => "col-8 header_padding"}
    %h1{:class => "header_text"}= params[:name]

%section{:class => "row"}
  %div{:class => "col-12 box_outline"}
    %table#items{:border => "1"}
      %thead
        %tr
          %th{:class => "gray"} Day
          %th{:class => "gray"} Month
      %tbody
        %tr{:class => "hi"}
          %td{:class => "padding"}= @Day
          %td{:class => "padding"}= @Month
        